<div class='col-md-4 col-md-offset-4 text-center panel panel-default'>
  <h1><%= t('profile.update') %></h1>

  <%= simple_form_for @user do |f| %>
    <%= render partial: 'form', locals: {f: f} %>
    <br>
    <%= f.button :submit, t('profile.save'), class: 'btn btn-success btn-block' %>
    <br>
  <% end %>

</div>
